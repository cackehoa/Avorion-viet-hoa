msgstr "¢${money}"

#: ./data/scripts/entity/merchants/scrapyard.lua:
msgid "Please, do come again."
msgstr "Vui lòng, làm lại."

#: ./data/scripts/entity/merchants/scrapyard.lua:
msgid "Remaining license time: ${time} Minutes"
msgstr "Thời gian cấp phép còn lại: ${time} Phút"

#: ./data/scripts/entity/merchants/scrapyard.lua:
msgid "You don't have a valid license."
msgstr "Bạn không có giấy phép hợp lệ."

#: ./data/scripts/entity/merchants/scrapyard.lua:
msgid "Your salvaging license will run out in 10 seconds."
msgstr "Giấy phép tái chế của bạn sẽ hết sau 10 giây."

#: ./data/scripts/entity/merchants/scrapyard.lua:
msgid "Your salvaging license in %s will run out in 10 seconds."
msgstr "Giấy phép tái chế của bạn trong %s sẽ hết sau 10 giây."

#: ./data/scripts/entity/merchants/scrapyard.lua:
msgid "Your salvaging license will run out in 20 seconds."
msgstr "Giấy phép tái chế của bạn sẽ hết sau 20 giây."

#: ./data/scripts/entity/merchants/scrapyard.lua:
msgid "Your salvaging license in %s will run out in 20 seconds."
msgstr "Giấy phép tái chế của bạn trong %s sẽ hết sau 20 giây."

#: ./data/scripts/entity/merchants/scrapyard.lua:
